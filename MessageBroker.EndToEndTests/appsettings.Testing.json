{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MessageBroker": {
    "Wal": {
      "Directory": "",
      "ResetOnStartup": true,
      "MaxWriteCountPerFile": 50,
      "FileNaming": {
        "Extension": "log",
        "EnqueuePrefix": "enqueue",
        "AckPrefix": "ack",
        "DeadPrefix": "dead",
        "EnqueueMergedPrefix": "enqueue-merged",
        "AckMergedPrefix": "ack-merged",
        "DeadMergedPrefix": "dead-merged"
      },
      "Manifest": {
        "FileName": "manifest.json"
      }
    },
    "Broker": {
      "Message": {
        "MaxPayloadSize": 1048576,
        "MaxDeliveryAttempts": 5
      },
      "Requeue": {
        "RequeueInterval": "00:00:00.500"
      },
      "ExpiredPolicy": {
        "ExpirationTime": "00:00:01"
      }
    },
    "GarbageCollector": {
      "CollectInterval": "00:00:01"
    }
  }
}