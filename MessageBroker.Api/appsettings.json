{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MessageBroker": {
    "Wal": {
      "Directory": "broker_data",
      "ResetOnStartup": false,
      "MaxWriteCountPerFile": 1000,
      "FileNaming": {
        "Extension": "log",
        "EnqueuePrefix": "enqueue",
        "AckPrefix": "ack",
        "DeadPrefix": "dead",
        "MergePrefix": "merge"
      },
      "Manifest": {
        "FileName": "manifest.json"
      }
    },
    "Broker": {
      "Message": {
        "MaxPayloadSize": 1048576,
        "MaxDeliveryAttempts": 5
      },
      "Requeue": {
        "RequeueInterval": "00:01:00"
      },
      "ExpiredPolicy": {
        "ExpirationTime": "00:02:00"
      }
    }
  }
}